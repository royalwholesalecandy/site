<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="wanexo_navigation">
         <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="label" xsi:type="string" translate="true">Wanexo Navigation</item>
                <item name="collapsible" xsi:type="boolean">true</item>
                <item name="sortOrder" xsi:type="number">100</item>
            </item>
        </argument>
        <field name="wan_navtype">
            <argument name="data" xsi:type="array">
            <item name="options" xsi:type="object">Wanexo\Mdlnavi\Model\Source\Bannertype</item>
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">60</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">select</item>
                    <item name="label" xsi:type="string" translate="true">Menu Type</item>
                </item>
            </argument>
        </field>
        <field name="wan_labelcolor">
            <argument name="data" xsi:type="array">
            <item name="options" xsi:type="object">Wanexo\Mdlnavi\Model\Source\Labelcolor</item>
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">70</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">select</item>
                    <item name="label" xsi:type="string" translate="true">Label Color</item>
                </item>
            </argument>
        </field>
         <field name="wan_label">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">80</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">input</item>
                    <item name="label" xsi:type="string" translate="true">Label</item>
                </item>
            </argument>
        </field>
        <field name="wan_topblock">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">90</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">textarea</item>
                    <item name="label" xsi:type="string" translate="true">Top Block</item>
                </item>
            </argument>
        </field>
        <field name="wan_bottomblock">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">100</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">textarea</item>
                    <item name="label" xsi:type="string" translate="true">Bottom Block</item>
                </item>
            </argument>
        </field>
        <field name="wan_rightblock">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">110</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">textarea</item>
                    <item name="label" xsi:type="string" translate="true">Right Block</item>
                </item>
            </argument>
        </field>
        <field name="wan_mblockwidth">
            <argument name="data" xsi:type="array">
            <item name="options" xsi:type="object">Wanexo\Mdlnavi\Model\Source\Mblockwidth</item>
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">120</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">select</item>
                    <item name="label" xsi:type="string" translate="true">Main Block Width</item>
                </item>
            </argument>
        </field>
         <field name="wan_rblockwidth">
            <argument name="data" xsi:type="array">
            <item name="options" xsi:type="object">Wanexo\Mdlnavi\Model\Source\Rblockwidth</item>
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">130</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">select</item>
                    <item name="label" xsi:type="string" translate="true">Right Block Width</item>
                </item>
            </argument>
        </field>
         <field name="wan_subcat">
            <argument name="data" xsi:type="array">
            <item name="options" xsi:type="object">Magento\Eav\Model\Entity\Attribute\Source\Boolean</item>
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">130</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">select</item>
                    <item name="label" xsi:type="string" translate="true">Enable Subcategories</item>
                </item>
            </argument>
        </field>
          <field name="wan_nocol">
            <argument name="data" xsi:type="array">
            <item name="options" xsi:type="object">Wanexo\Mdlnavi\Model\Source\Nocol</item>
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">140</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">select</item>
                    <item name="label" xsi:type="string" translate="true">Select number of column</item>
                </item>
            </argument>
        </field>
        <field name="wan_thumbnail">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="source" xsi:type="string">category</item>
                    <item name="label" xsi:type="string" translate="true">Menu Thumbnail</item>
                    <item name="visible" xsi:type="boolean">true</item>
                    <item name="formElement" xsi:type="string">fileUploader</item>
                    <item name="elementTmpl" xsi:type="string">ui/form/element/uploader/uploader</item>
                    <item name="previewTmpl" xsi:type="string">Magento_Catalog/image-preview</item>
                    <item name="required" xsi:type="boolean">false</item>
                    <item name="sortOrder" xsi:type="number">150</item>
                    <item name="uploaderConfig" xsi:type="array">
                       <item name="url" xsi:type="url" path="mdlnavi/category_thumbimage/upload"/>
                    </item>
                </item>
            </argument>
        </field>
        <field name="wan_icon">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="source" xsi:type="string">category</item>
                    <item name="label" xsi:type="string" translate="true">Menu Icon</item>
                    <item name="visible" xsi:type="boolean">true</item>
                    <item name="formElement" xsi:type="string">fileUploader</item>
                    <item name="elementTmpl" xsi:type="string">ui/form/element/uploader/uploader</item>
                    <item name="previewTmpl" xsi:type="string">Magento_Catalog/image-preview</item>
                    <item name="required" xsi:type="boolean">false</item>
                    <item name="sortOrder" xsi:type="number">150</item>
                    <item name="uploaderConfig" xsi:type="array">
                        <item name="url" xsi:type="url" path="mdlnavi/category_iconimage/upload"/>
                    </item>
                </item>
            </argument>
        </field>
         <field name="bg_position">
            <argument name="data" xsi:type="array">
            <item name="options" xsi:type="object">Wanexo\Mdlnavi\Model\Source\Bgposition</item>
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">65</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">select</item>
                    <item name="label" xsi:type="string" translate="true">BG Position</item>
                </item>
            </argument>
        </field>
          <field name="bgimage">
               <argument name="data" xsi:type="array">
                   <item name="config" xsi:type="array">
                       <item name="dataType" xsi:type="string">string</item>
                       <item name="source" xsi:type="string">category</item>
                       <item name="label" xsi:type="string" translate="true">Megamenu Background Image</item>
                       <item name="visible" xsi:type="boolean">true</item>
                       <item name="formElement" xsi:type="string">fileUploader</item>
                       <item name="elementTmpl" xsi:type="string">ui/form/element/uploader/uploader</item>
                       <item name="previewTmpl" xsi:type="string">Magento_Catalog/image-preview</item>
                       <item name="required" xsi:type="boolean">false</item>
                       <item name="sortOrder" xsi:type="number">63</item>
                       <item name="uploaderConfig" xsi:type="array">
                           <item name="url" xsi:type="url" path="mdlnavi/category_bgimage/upload"/>
                       </item>
                   </item>
               </argument>
          </field>
          <field name="custom_link">
             <argument name="data" xsi:type="array">
             <item name="options" xsi:type="object">Wanexo\Mdlnavi\Model\Source\CustomLink</item>
                 <item name="config" xsi:type="array">
                     <item name="sortOrder" xsi:type="number">70</item>
                     <item name="dataType" xsi:type="string">string</item>
                     <item name="formElement" xsi:type="string">select</item>
                     <item name="label" xsi:type="string" translate="true">Page/Custom Links</item>
                 </item>
             </argument>
          </field>
          <field name="wan_clink">
               <argument name="data" xsi:type="array">
                   <item name="config" xsi:type="array">
                       <item name="sortOrder" xsi:type="number">71</item>
                       <item name="dataType" xsi:type="string">string</item>
                       <item name="formElement" xsi:type="string">input</item>
                       <item name="label" xsi:type="string" translate="true">Custom Links</item>
                   </item>
               </argument>
           </field>
          <field name="wan_page">
             <argument name="data" xsi:type="array">
             <item name="options" xsi:type="object">Magento\Cms\Model\Config\Source\Page</item>
                 <item name="config" xsi:type="array">
                     <item name="sortOrder" xsi:type="number">72</item>
                     <item name="dataType" xsi:type="string">string</item>
                     <item name="formElement" xsi:type="string">select</item>
                     <item name="label" xsi:type="string" translate="true">Page Link</item>
                 </item>
             </argument>
          </field>
    </fieldset>
</form>