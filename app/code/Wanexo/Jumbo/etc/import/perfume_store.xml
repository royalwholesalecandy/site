<root>
    <config>
		<web>
			<default>
				<cms_home_page>jumbo_home_perfume</cms_home_page>
				<cms_no_route>no-route</cms_no_route>
			</default>
		</web>
		
		<cms>
			<wysiwyg>
				<enabled>hidden</enabled>
			</wysiwyg>
		</cms>
		
		<wanexo_themeoption>
			<genral_setting>
				<navfix>1</navfix>
				<boxed></boxed>
				<wxo_heading>5</wxo_heading>
			</genral_setting>
			
			<wxo_header_setting>
				<wxo_header>4</wxo_header>
			</wxo_header_setting>
		
			<wxo_footer_setting>
				<wxo_footer>4</wxo_footer>
			</wxo_footer_setting>
		</wanexo_themeoption>
		
		<wanexo_np>
			<general>
				<status>1</status>
				<npImageSrc>default/fashion_newsletter.jpg</npImageSrc>
				<np_bg_repeat>no-repeat</np_bg_repeat>
				<np_bg_bp>center center</np_bg_bp>
			</general>
		</wanexo_np>
		
		<wanexo_productcollection>
			<wxo_category_settings>
				<wxo_itemtype>4</wxo_itemtype>
				<wxo_itempro>col-lg-4 col-md-3 col-sm-4 col-xs-6 clear-item-3</wxo_itempro>
			</wxo_category_settings>
		</wanexo_productcollection>
		
		<jumbo_design>
			<general>
				<theme_color>ff9ab1</theme_color>
				<enable></enable>
				<body_color></body_color>
				<wxo_bg></wxo_bg>
				<wxo_bg_sz></wxo_bg_sz>
				<wxo_bg_br></wxo_bg_br>
				<wxo_bg_bp>center center</wxo_bg_bp>
				<wxo_bg_atm>scroll</wxo_bg_atm>
				<enable2></enable2>
				<wrapper_bg_color></wrapper_bg_color>
			</general>
			
			<font>
				<enable>1</enable>
				<font_family>google</font_family>
				<google_font_family>Didact Gothic</google_font_family>
			</font>
			
			<header>
				<enable>1</enable>
				<header_bg></header_bg>
				<header_qty_bg></header_qty_bg>
				<header_cart_bg></header_cart_bg>
				<header_cart_border></header_cart_border>
				<header_search_bg></header_search_bg>
				<header_search_border></header_search_border>
				<header_search_btn_bg></header_search_btn_bg>
				<nav_border></nav_border>
				<nav_text_hover_color></nav_text_hover_color>
				<nav_heading>ffffff</nav_heading>
				<nav_heading_hover>545454</nav_heading_hover>
				<nav_heading_icon>ffffff</nav_heading_icon>
				<nav_heading_icon_hover>545454</nav_heading_icon_hover>
			</header>
			
			
			
			<footer>
				<enable></enable>
				<footerType></footerType>
				<ft_background></ft_background>
				<footer_bg></footer_bg>
				<wxo_bg_br></wxo_bg_br>
				<wxo_bg_bp>center center</wxo_bg_bp>
				<wxo_bg_atm>scroll</wxo_bg_atm>
				<wxo_bg_sz></wxo_bg_sz>
			</footer>
			
		</jumbo_design>
		
		<wanexo_mlayer>
			<banner>
				<bannertype>3</bannertype>
				<full_banner>1</full_banner>
				<bannercontent>0</bannercontent>
				<banner_block></banner_block>
				<bannerpagination>1</bannerpagination>
			</banner>
		</wanexo_mlayer>
	</config>
</root>