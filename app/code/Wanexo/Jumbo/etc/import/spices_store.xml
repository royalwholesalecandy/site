<root>
    <config>
		<web>
			<default>
				<cms_home_page>jumbo_home_spices</cms_home_page>
				<cms_no_route>no-route</cms_no_route>
			</default>
		</web>
		
		<cms>
			<wysiwyg>
				<enabled>hidden</enabled>
			</wysiwyg>
		</cms>
		
		<wanexo_themeoption>
			<genral_setting>
				<navfix>1</navfix>
				<boxed>1</boxed>
				<wxo_heading>2</wxo_heading>
			</genral_setting>
			
			<wxo_header_setting>
				<wxo_header>1</wxo_header>
			</wxo_header_setting>
		
			<wxo_footer_setting>
				<wxo_footer>3</wxo_footer>
			</wxo_footer_setting>
		</wanexo_themeoption>
		
		<wanexo_np>
			<general>
				<status>1</status>
				<npImageSrc>default/fashion_newsletter.jpg</npImageSrc>
				<np_bg_repeat>no-repeat</np_bg_repeat>
				<np_bg_bp>center center</np_bg_bp>
			</general>
		</wanexo_np>
		
		<wanexo_productcollection>
			<wxo_category_settings>
				<wxo_itemtype>4</wxo_itemtype>
				<wxo_itempro>col-lg-4 col-md-3 col-sm-4 col-xs-6 clear-item-3</wxo_itempro>
			</wxo_category_settings>
		</wanexo_productcollection>
		
		<jumbo_design>
			<general>
				<theme_color>ffb400</theme_color>
				<enable>1</enable>
				<body_color>282828</body_color>
				<wxo_bg>default/spice_bg.jpg</wxo_bg>
				<wxo_bg_sz>cover</wxo_bg_sz>
				<wxo_bg_br>no-repeat</wxo_bg_br>
				<wxo_bg_bp>top left</wxo_bg_bp>
				<wxo_bg_atm>scroll</wxo_bg_atm>
				<enable2>1</enable2>
				<wrapper_bg_color>ffffff</wrapper_bg_color>
			</general>
			
			<font>
				<enable></enable>
				<font_family></font_family>
				<google_font_family></google_font_family>
			</font>
			
			<header>
				<enable>1</enable>
				<header_bg>4e463b</header_bg>
				<header_qty_bg></header_qty_bg>
				<header_cart_bg>729861</header_cart_bg>
				<header_cart_border>729861</header_cart_border>
				<header_search_bg>ffffff</header_search_bg>
				<header_search_border>ffffff</header_search_border>
				<header_search_btn_bg>ffb400</header_search_btn_bg>
				<nav_border></nav_border>
				<nav_text_hover_color></nav_text_hover_color>
				<nav_heading></nav_heading>
				<nav_heading_hover></nav_heading_hover>
				<nav_heading_icon></nav_heading_icon>
				<nav_heading_icon_hover></nav_heading_icon_hover>
			</header>
			
			<footer>
				<enable></enable>
				<footerType></footerType>
				<ft_background></ft_background>
				<footer_bg></footer_bg>
				<wxo_bg_br></wxo_bg_br>
				<wxo_bg_bp>center center</wxo_bg_bp>
				<wxo_bg_atm>scroll</wxo_bg_atm>
				<wxo_bg_sz></wxo_bg_sz>
			</footer>
			
		</jumbo_design>
		
		<wanexo_mlayer>
			<banner>
				<bannertype>1</bannertype>
				<full_banner>1</full_banner>
				<bannercontent>0</bannercontent>
				<banner_block></banner_block>
				<bannerpagination></bannerpagination>
			</banner>
		</wanexo_mlayer>
	</config>
</root>