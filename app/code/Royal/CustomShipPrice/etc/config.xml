        <royal_customshipprice>
            <general>
                <module_name>Custom Shipping Price</module_name>
                <module_page_link>https://royal.com/custom-shipping-price-2.html</module_page_link>
            </general>
        </royal_customshipprice>
        <carriers>
            <customshipprice>
                <active>1</active>
